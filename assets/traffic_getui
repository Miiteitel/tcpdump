pkg="zx.getui[0-9].pushservice";iface="rmnet0|wlan0";ps=`/system/bin/busybox ps | /system/bin/busybox awk '/'"$pkg"'/ {print $1}'|/system/bin/busybox awk '{if(NR==1)print $1}'`;echo "pkg=$pkg""\n""ps=$ps";uid=`/system/bin/busybox awk '/Uid/ {print $2}' /proc/$ps/status`;echo uid=$uid;/system/bin/busybox awk '($4 == uid && ($5 == "0" || $5 == "1") && /'"$iface"'/) {print $0 "\n" "rcv=" $6 ", packet=" $7 "\n" "snd=" $8 ", packet=" $9 "\n" "total=" ($6+$8)}' uid=$uid /proc/net/xt_qtaguid/stats