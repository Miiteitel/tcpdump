pkg="bdservice_v1";iface="rmnet0|wlan0";ps=`ps | awk '/'"$pkg"'/ {print $2}'`;echo "pkg=$pkg""\n""ps=$ps";uid=`awk '/Uid/ {print $2}' /proc/$ps/status`;echo uid=$uid;awk '($4 == uid && $5 == "0" && /'"$iface"'/) {print $0 "\n" "rcv=" $6 "; packet=" $7 "\n" "snd=" $8 "; packet=" $9 "\n" "total=" ($6+$8)}' uid=$uid /proc/net/xt_qtaguid/stats